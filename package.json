    "apollo-link-prismic": "^1.0.8",
    "codegen-prismic-fetch": "^1.0.3",
    "dayjs": "^1.10.4",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "less": "^4.1.1",
    "next": "^11.1.0",
    "plaiceholder": "^2.0.1",
    "prismic-reactjs": "^1.3.3",
    "query-string": "^7.0.0",
    "react": "17.0.2",
    "react-div-100vh": "^0.6.0",
    "react-dom": "17.0.2",
    "react-hamburger-menu": "^1.2.1",
