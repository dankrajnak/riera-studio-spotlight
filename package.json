    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql-tag": "^2.11.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,md}": "prettier --write"
  }
}
